Private Sub cboReportName_AfterUpdate()
'Enable filters based on report selected

Me.cmdReport.Enabled = Not IsNull(Me.cboReportName)
Select Case Me.cboReportName.Column(2)
    Case "CameraPullChecklist"
        'Location filter only
        Me.chkDateFilter = False
        Me.chkDateFilter.Enabled = False
        Me.chkLocationFilter.Enabled = True
    Case "CameraSummary", "CameraSummaryNoSp"
        'Date filter only
        Me.chkLocationFilter = False
        Me.chkDateFilter.Enabled = True
        Me.chkLocationFilter.Enabled = False
    Case "IndDetSummary"
        'Both enabled
        Me.chkDateFilter.Enabled = True
        Me.chkLocationFilter.Enabled = True
    Case Else
        'No filters available
        Me.chkDateFilter = False
        Me.chkLocationFilter = False
        Me.chkDateFilter.Enabled = False
        Me.chkLocationFilter.Enabled = False
End Select

Call chkLocationFilter_AfterUpdate
Call chkDateFilter_AfterUpdate

End Sub

Private Sub chkDateFilter_AfterUpdate()
'Enable dropdown when box is checked

Me.cboSeasonFilter.Enabled = Me.chkDateFilter.Value

End Sub

Private Sub chkLocationFilter_AfterUpdate()
'Enable dropdown when box is checked

Me.cboStudyAreaFilter.Enabled = Me.chkLocationFilter.Value

End Sub

