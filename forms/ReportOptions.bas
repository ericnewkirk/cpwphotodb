'Display help message

HelpMsg Me.Name, Me.Name & " Form Help"

End Sub

Private Sub cmdReport_Click()
'Open the selected report

DoCmd.OpenReport Me.cboReportName.Column(2), acViewPreview, , _
    GetFilter()

End Sub

'------------------------------
'Form-specific custom functions
'------------------------------

Private Function GetFilter() As String
'Create filter string if appropriate

Dim strFilter As String

strFilter = ""

If Me.chkDateFilter.Value = True And _
    IsNull(Me.cboSeasonFilter) = False Then
    'Get field season filter string
    strFilter = "((Year(FilterDate))=" & _
        Me.cboSeasonFilter & ") AND "
End If

If Me.chkLocationFilter.Value = True And _
    IsNull(Me.cboStudyAreaFilter) = False Then
    'Get study area filter string
    strFilter = strFilter & "((StudyAreaID)=" & _
        Me.cboStudyAreaFilter & ")"
End If

If Right(strFilter, 5) = " AND " Then
    strFilter = Left(strFilter, Len(strFilter) - 5)
End If

GetFilter = strFilter

End Function
