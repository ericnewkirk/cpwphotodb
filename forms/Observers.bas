Option Compare Database
Option Explicit

Private Const strGenericError As String = _
    "The Observers form encountered an error."

Private Sub cmdCancel_Click()
'Cancel saving record and close

On Error GoTo ErrHandler

Me.Undo
DoCmd.Close
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

Private Sub cmdDone_Click()
'Save record and close

On Error GoTo ErrHandler

If Me.Dirty Then Me.Dirty = False
DoCmd.Close
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

Private Sub Form_AfterUpdate()
'Hide cancel and done buttons

On Error GoTo ErrHandler

If Me.cmdCancel.Visible Then
    Me.FirstName.SetFocus
    Me.cmdCancel.Visible = False
    Me.cmdDone.Visible = False
End If
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

Private Sub Form_Current()
'Hide cancel and done buttons

On Error Resume Next

Me.cmdCancel.Visible = False
Me.cmdDone.Visible = False

End Sub

Private Sub Form_Open(Cancel As Integer)
'Use openargs to populate new record when called from
'PhotoID login form

Dim i As Integer

On Error GoTo ErrHandler

If Len(Me.OpenArgs) > 0 Then
    DoCmd.GoToRecord , , acNewRec
    Me.cmdCancel.Visible = True
    Me.cmdDone.Visible = True
    If Left(Me.OpenArgs, 1) = "*" Then
        Me.Initials = Mid(Me.OpenArgs, 2)
    Else
        i = InStr(1, Me.OpenArgs, " ")
        If i > 1 Then
            Me.FirstName = Left(Me.OpenArgs, i - 1)
            Me.LastName = Mid(Me.OpenArgs, i + 1)
            Me.Initials = UCase(Left(Me.OpenArgs, 1)) & "X" & _
                UCase(Mid(Me.OpenArgs, i + 1, 1))
        Else
            Me.FirstName = Me.OpenArgs
        End If
    End If
    MsgBox "The new data has been added to the form." & vbNewLine & _
        "Please fill in the rest of the information." & vbNewLine & _
        vbNewLine & "If you don't want to add this observer, press cancel."
End If
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

