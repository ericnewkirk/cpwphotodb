Private Function BuildOccString() As String
'Create expressions for the occasion fields in the
'occupancy query

Dim lngSpeciesID As Long
Dim iDuration As Integer
Dim iBuffer As Integer
Dim iOccasions As Integer
Dim bGroup As Boolean
Dim b2Obs As Boolean
Dim strOcc As String
Dim i As Integer
Dim j As Integer

'Get parameters
lngSpeciesID = Nz(Me.cboSpecies, 0)
iDuration = Me.txtIntDuration
If Me.cboIntDurationUnits = "Week(s)" Then
    iDuration = iDuration * 7
End If
iBuffer = Nz(Me.txtBuffer, 0)
If Me.cboBufferUnits = "Week(s)" Then
    iBuffer = iBuffer * 7
End If
iOccasions = Nz(Me.txtIntNumber, 1)
bGroup = (InStr(1, Me.cboSpecies.RowSource, "GroupID") > 0)
b2Obs = Me.chk2Obs

strOcc = ""

'Add string for each occasion
For i = 0 To iOccasions - 1
    j = i * (iBuffer + iDuration)
    strOcc = strOcc & "GetOccasion([CameraLocations].[LocationID]," & _
        "DateAdd('d'," & j & ",[StartDateTime]),DateAdd('s',-1," & _
        "DateAdd('d'," & j + iDuration & ", [StartDateTime]))," & _
        lngSpeciesID & ", " & bGroup & ", " & b2Obs & _
        ") AS Occasion" & i + 1 & ", "
Next i

BuildOccString = strOcc

End Function

Private Sub BuildQuery()
'Generate SQL statement and update OccupancyQuery

Dim strSQL As String
Dim qdf As QueryDef

'Get SQL for occasions
strSQL = BuildOccString

'Add other fields
strSQL = "SELECT [StudyAreas].[StudyAreaAbbr] & ' - ' & " & _
    "[CameraLocations].[LocationName] AS Location, " & _
    strSQL & "1 As Group1, CameraLocations.StudyAreaID AS " & _
    "StudyArea, Year([Visits].[VisitDate]) AS " & _
    "SurveyYear, "

If Me.optSpatial = 1 Then
    strSQL = strSQL & "Nz(CameraLocations.UTM_E,'.') " & _
    "AS CameraUTM_E, Nz(CameraLocations.UTM_N,'.') " & _
    "AS CameraUTM_N, Nz(CameraLocations.UTMZone,'.') " & _
    "AS CameraUTM_Zone, "
Else
    strSQL = strSQL & "Nz(CameraLocations.LatitudeDD,'.') " & _
    "AS CameraLat, Nz(CameraLocations.LongitudeDD,'.') " & _
    "AS CameraLong, "
End If

'Add startdatetime field
strSQL = strSQL & BuildStartString()

'Add from clause
strSQL = strSQL & "FROM (StudyAreas INNER JOIN " & _
    "CameraLocations ON StudyAreas.StudyAreaID = " & _
    "CameraLocations.StudyAreaID) INNER JOIN Visits ON " & _
    "CameraLocations.LocationID = Visits.LocationID "

'Add where clause
strSQL = strSQL & BuildWhere()

'Add order by clause
strSQL = strSQL & " ORDER BY CameraLocations.LocationID, " & _
    "Visits.VisitDate;"

'Update OccupancyQuery with new SQL
Set qdf = CurrentDb.QueryDefs("OccupancyQuery")
qdf.SQL = strSQL
Set qdf = Nothing

End Sub

Private Function BuildStartString() As String
'Generate SQL for StartDatetime field

Dim dblDelay As Double
Dim strDateField As String
Dim dStart As Date
Dim lngTotalTime As Long

Const strOccStart As String = _
    "Nz((SELECT Min(V.ActiveStart) FROM Visits AS V " & _
    "WHERE (((V.SetVisitID) = [Visits].[VisitID])))," & _
    "[Visits].[VisitDate])"

If Me.optStartDate = 1 Then
    'Relative Start, get delay and start time
    dblDelay = Nz(Me.txtDaysAfterStart, 0) + _
        Nz(Me.txtStartTime, 0)
    If dblDelay > 0 Then
        'Strip time from occasionstart field and
        'add specified number of days and start time
        strDateField = "CDate(DateValue(" & _
            strOccStart & ") + " & _
            dblDelay & ")"
    Else
        'No delay or start time specified,
        'use occasionstart field
        strDateField = strOccStart
    End If
Else
    'Absolute start, use date specified (corrected for year)
    dStart = Nz(Me.txtStartDate, 0)
    dStart = dStart + Nz(Me.txtStartTime, 0)
    'Calculate total sampling time
    lngTotalTime = Me.txtIntDuration
    If Left(Me.cboIntDurationUnits, 1) = "W" Then
        lngTotalTime = lngTotalTime * 7
    End If
    lngTotalTime = lngTotalTime * Me.txtIntNumber
    If Left(Me.cboBufferUnits, 1) = "W" Then
        lngTotalTime = lngTotalTime + _
            (Me.txtBuffer * 7 * (Me.txtIntNumber - 1))
    Else
        lngTotalTime = lngTotalTime + _
            (Me.txtBuffer * (Me.txtIntNumber - 1))
    End If
    'Get SQL for field
    strDateField = "DateAdd('yyyy', Year(" & strOccStart & _
        ") - " & Year(dStart) & " + IIf(DatePart('y', " & _
        strOccStart & ") > " & DatePart("y", dStart) + _
        lngTotalTime & ", 1, 0), #" & USDate(dStart) & "#)"
End If

strDateField = strDateField & " AS StartDateTime "
BuildStartString = strDateField

End Function

Private Function BuildWhere() As String
'Returns the where clause for OccupancyQuery

Dim strWhere As String

strWhere = "WHERE (((Visits.VisitTypeID) = 3) AND "

'Add filters one at a time
If Not IsNull(Me.cboStudyArea) Then
    strWhere = strWhere & "((CameraLocations.StudyAreaID) = " & _
        Me.cboStudyArea & ") AND "
End If

If Not IsNull(Me.txtNorthOfUTM) Then
    strWhere = strWhere & "((CameraLocations.UTM_N) >= " & _
        Me.txtNorthOfUTM & ") AND "
End If

If Not IsNull(Me.txtSouthOfUTM) Then
    strWhere = strWhere & "((CameraLocations.UTM_N) <= " & _
        Me.txtSouthOfUTM & ") AND "
End If

If Not IsNull(Me.txtEastOfUTM) Then
    strWhere = strWhere & "((CameraLocattions.UTM_E) >= " & _
        Me.txtEastOfUTM & ") AND "
End If

If Not IsNull(Me.txtWestOfUTM) Then
    strWhere = strWhere & "((CameraLocations.UTM_E) <= " & _
        Me.txtWestOfUTM & ") AND "
End If

If Not IsNull(Me.cboSurveyYear) Then
    strWhere = strWhere & "((Year(Visits.VisitDate)) = " & _
        Me.cboSurveyYear & ") AND "
End If

'Remove last "and" and finish off where clause
strWhere = Left(strWhere, Len(strWhere) - 5) & ") "

BuildWhere = strWhere

End Function

Private Function DisableButtons() As Integer
'Disable output buttons when settings have changed

Dim strAC As String

strAC = Me.ActiveControl.Name

If strAC = "cmdQuery" Or strAC = "cmdSave" Or _
    strAC = "cmdExport" Then
