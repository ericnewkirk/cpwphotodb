Private Sub Form_Unload(Cancel As Integer)
'Check for blanks and provide warning message

Dim strMsg As String

If DCount("*", "CameraLocations", "APZone Is Null") > 0 Then
    strMsg = "There are locations with no zone assigned." & vbNewLine & _
        vbNewLine & "If you press OK and run your query with the custom " & _
        "zones option, these locations will be omitted."
    If MsgBox(strMsg, vbOKCancel, "Blank Activity Zones") = vbOK Then
        Cancel = False
    Else
        Cancel = True
    End If
End If

End Sub
