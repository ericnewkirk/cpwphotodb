Option Compare Database
Option Explicit

Private Const strGenericError As String = _
    "The IndividualID form encountered an error."

Private Sub cmdDelete_Click()
'Delete the current record

On Error GoTo ErrHandler

'Delete record
If Me.NewRecord Then
    Me.Undo
Else
    DoCmd.RunCommand acCmdDeleteRecord
End If

'Requery subform
Me.Requery
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

Private Sub Form_Current()
'Limit combo box to individuals of the correct species

Me.IndividualID.Requery

End Sub

Private Sub IndividualID_NotInList(NewData As String, _
    Response As Integer)
'Handle values in combo box that don't match list

On Error GoTo ErrHandler

'Prompt to add new individual to table
If MsgBox(NewData & " is not in the individuals list." & vbNewLine & _
    "Do you want to add it?", vbYesNo, "Unknown Individual") = vbYes Then
    'Allow new data in list and open form to add to table
    Response = acDataErrAdded
    DoCmd.OpenForm "Individuals", acNormal, , , , acDialog, _
        NewData & ";" & Me.Parent.SpeciesID
    Me.Parent.cboRefPhotos.Requery
Else
    'User chose not to add, undo typed entry
    Response = acDataErrContinue
    Me.Undo
End If
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

Private Sub lnkSetRefPhoto_Click()
'Open popup to set reference photo

If Not IsNull(Me.IndividualID) Then
    DoCmd.OpenForm "SetRefPhotoPopup", , , "IndividualID=" & _
        Me.IndividualID, , acDialog, Me.Parent!IndDetFilmStrip.Form!ImgPath
End If
Me.IndividualID.SetFocus

End Sub
