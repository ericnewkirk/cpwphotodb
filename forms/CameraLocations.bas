Private Sub cmdSwitchCoord_Click()
'Changes between UTM and Lat/Long fields

Dim bUTM As Boolean

Me.cmdSwitchCoord.SetFocus

bUTM = (InStr(1, Me.cmdSwitchCoord.Caption, "UTM") > 0)

Me.UTM_E.Visible = bUTM
Me.UTM_N.Visible = bUTM
Me.UTMZone.Visible = bUTM
Me.UTMHemisphere.Visible = bUTM
Me.UTM_E_Label.Visible = bUTM
Me.UTM_N_Label.Visible = bUTM
Me.UTMZone_Label.Visible = bUTM
Me.UTMHemisphere_Label.Visible = bUTM
Me.LatitudeDD.Visible = Not bUTM
Me.LongitudeDD.Visible = Not bUTM
Me.LatitudeDD_Label.Visible = Not bUTM
Me.LongitudeDD_Label.Visible = Not bUTM

If bUTM Then
    Me.UTM_E.SetFocus
    Me.cmdSwitchCoord.Caption = "Switch to Lat/Long"
Else
    Me.LatitudeDD.SetFocus
    Me.cmdSwitchCoord.Caption = "Switch to UTM"
End If

End Sub

Private Sub Form_BeforeDelConfirm(Cancel As Integer, _
