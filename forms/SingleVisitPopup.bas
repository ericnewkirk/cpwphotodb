Option Compare Database
Option Explicit

Private Const strGenericError As String = _
    "The Visits form encountered an error."

Private Sub cmdDelete_Click()
'Deletes the current visit and associated data

Dim lngVisitID As Long

On Error GoTo ErrHandler

lngVisitID = Me.VisitID

'Check for photos
If DCount("*", "Photos", "VisitID=" & lngVisitID) > 0 Then
    'Deal with photos through popup form
    DoCmd.OpenForm "DeleteVisitPhotos", , , , , acDialog, lngVisitID
Else
    'Just delete the visit
    DoCmd.SetWarnings False
    DoCmd.RunCommand acCmdDeleteRecord
    DoCmd.SetWarnings True
End If

'Check if visit was actually deleted
Me.RecordsetClone.FindFirst "VisitID=" & lngVisitID
If Me.RecordsetClone.NoMatch Then
    'Visit was deleted, return to visit list
    DoCmd.Close acForm, "SingleVisitPopup", acSaveNo
End If

Exit Sub

ErrHandler:
    ErrorMsg "An error occurred while deleting the visit.", _
        Err.Number, Err.Description, Me.Name
    DoCmd.SetWarnings True

End Sub

Private Sub cmdHelp_Click()
'Display help message

HelpMsg Me.Name, Me.Name & " Form Help"

End Sub

Private Sub Form_Current()
'Enable/disable navigation buttons based on current record

On Error GoTo ErrHandler

Me.VisitType.SetFocus

Call VisitType_AfterUpdate

Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description, Me.Name

End Sub

Private Sub linkActiveDates_Click()
'Open popup form to edit dates

DoCmd.OpenForm "EditActiveDatesSingle", , , "VisitID=" & Me.VisitID, _
    , acDialog

End Sub

Private Sub linkPhotoDate_Click()
'Open popup form to edit image timestamps

DoCmd.OpenForm "AdjustImageDates", , , "VisitID=" & Me.VisitID, _
    , acDialog

End Sub

Private Sub VisitType_AfterUpdate()
'Show/hide subform based on visit type

On Error GoTo ErrHandler

'Enable photos subform for visits other than setup
If IsNull(Me.VisitType) Or Me.VisitType = 3 Then
    Me.CamPhotosSubform.Visible = False
    Me.linkActiveDates.Visible = False
    Me.linkPhotoDate.Visible = False
Else
    Me.CamPhotosSubform.Visible = True
    If DCount("*", "Photos", "VisitID=" & Nz(Me.VisitID, 0)) > 0 Then
        Me.linkActiveDates.Visible = True
        Me.linkPhotoDate.Visible = True
    Else
        Me.linkActiveDates.Visible = False
        Me.linkPhotoDate.Visible = False
    End If
End If

Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description, Me.Name

End Sub

Private Sub VisitType_BeforeUpdate(Cancel As Integer)

On Error GoTo ErrHandler

If Me!VisitType = 3 Then
    If DCount("*", "Photos", "VisitID=" & Nz(Me.VisitID, 0)) > 0 Then
        Cancel = True
        Me!VisitType.Undo
        MsgBox "You cannot change this visit to a set " & _
            "because it is already associated with photos."
    End If
End If

Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description, Me.Name

End Sub
