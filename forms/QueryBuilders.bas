Option Compare Database
Option Explicit

Private Const strGenericError As String = _
    "The analysis selection form encountered an error."

Private Sub cboQueryType_AfterUpdate()
'Enable go button

On Error Resume Next

Me.cmdGo.Enabled = Not IsNull(Me.cboQueryType)
Me.cmdGo.SetFocus

End Sub

Private Sub cmdCancel_Click()
'Close form

On Error GoTo ErrHandler

DoCmd.Close
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

Private Sub cmdGo_Click()
'Open the appropriate query builder form

On Error GoTo ErrHandler

DoCmd.OpenForm Me.cboQueryType.Column(2)
DoCmd.Close acForm, "QueryBuilders"
Exit Sub

ErrHandler:
    ErrorMsg "An error occured opening the form.", Err.Number, _
        Err.Description

End Sub

Private Sub cmdHelp_Click()
'Display help message

HelpMsg Me.Name, Me.Name & " Form Help"

End Sub

Private Sub Form_Open(Cancel As Integer)
'Minimize switchboard

HideSwitchboard acForm, Me.Name

End Sub

Private Sub txtTypeReqd_Enter()
'Avoid focus

If Me.cmdGo.Enabled Then
    Me.cmdGo.SetFocus
Else
    Me.cboQueryType.SetFocus
End If

End Sub
