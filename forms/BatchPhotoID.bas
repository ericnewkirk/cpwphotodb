Private Sub ApplyIDs()
'Loop through selected photos and add ID where needed

Dim lngFirstImage As Long
Dim lngLastImage As Long
Dim lngCount As Long

Dim lngSpecies As Long
Dim vDet As Variant
Dim lngInd As Long
Dim vCom As Variant
Dim lngObs As Long

Dim db As DAO.Database
Dim strSQL As String
Dim rs As DAO.Recordset
Dim rsAdd As DAO.Recordset

On Error GoTo ErrHandler

DoCmd.SetWarnings False

'Get Image IDs from form
lngFirstImage = Me.cboFirstImage
lngLastImage = Me.cboLastImage

'Get records
Set db = CurrentDb
strSQL = db.QueryDefs("qryBatchIDRecSource").SQL
strSQL = Mid(strSQL, InStr(1, strSQL, "FROM "), _
    Len(strSQL) - InStr(1, strSQL, "FROM "))
strSQL = "SELECT ImageID " & strSQL
Set rs = db.OpenRecordset(strSQL)
If Not (rs.BOF And rs.EOF) Then
    rs.FindFirst "ImageID=" & lngFirstImage
    If Not rs.NoMatch Then
        Do Until rs.EOF
            lngCount = lngCount + 1
            If rs!ImageID = lngLastImage Then
                Exit Do
            Else
                rs.MoveNext
            End If
        Loop
    End If
End If

'Check that photos were found
If rs.EOF Then
    MsgBox "No photos are selected." & vbNewLine & vbNewLine & _
        "Make sure the order is correct."
    GoTo ApplyExit
End If

'Prompt to confirm
If MsgBox("Are you sure you want to apply IDs to " & _
    lngCount & " photos?", _
    vbOKCancel) = vbCancel Then
    GoTo ApplyExit
End If

'Get field values
lngSpecies = Me.SpeciesID
vDet = Me.DetailID
lngInd = Nz(Me.Individuals, 1)
vCom = Me.Comments
lngObs = GetObserver()

'Rip
Set rsAdd = db.OpenRecordset("SELECT * FROM Detections WHERE ImageID=0")
rs.MoveFirst
rs.FindFirst "ImageID=" & lngFirstImage
Do Until rs.EOF
    If Not CheckMatch(rs!ImageID, lngSpecies, vDet, lngInd, vCom, _
        lngObs, (Me.optMode = 2)) Then
        'No matching ID
        rsAdd.AddNew
        rsAdd!ImageID = rs!ImageID
        rsAdd!SpeciesID = lngSpecies
        rsAdd!Individuals = lngInd
        rsAdd!ObsID = lngObs
        rsAdd!DetailID = vDet
        rsAdd!Comments = vCom
        rsAdd.Update
    End If
    SetFlag rs!ImageID
    If rs!ImageID = lngLastImage Then
        Exit Do
    Else
        rs.MoveNext
    End If
Loop

rsAdd.Close
rs.Close

On Error Resume Next

'Jump to next record in PhotoID
Set rs = Forms("PhotoID").RecordsetClone
rs.FindFirst "ImageID=" & Me.cboLastImage
If Not (rs.NoMatch Or rs.EOF) Then
    rs.MoveNext
    Forms("PhotoID").Bookmark = rs.Bookmark
End If
rs.Close

DoCmd.Close acForm, "BatchPhotoID", acSaveNo

ApplyExit:
    DoCmd.SetWarnings True
    Set rsAdd = Nothing
    Set rs = Nothing
    Set db = Nothing
    Exit Sub

ErrHandler:
    ErrorMsg "Unable to apply ID.", Err.Number, Err.Description, Me.Name
    Resume ApplyExit

End Sub

Private Function CheckMatch(lngImageID As Long, lngSpecies As Long, _
    vDet As Variant, lngInd As Long, vCom As Variant, _
    lngObs As Long, bDelete As Boolean) As Boolean

Dim db As DAO.Database
Dim rsDet As DAO.Recordset

Set db = CurrentDb
Set rsDet = db.OpenRecordset("SELECT * FROM Detections WHERE ImageID=" & _
    lngImageID & " AND ObsID=" & lngObs & " AND StatusID<3")
If Not (rsDet.BOF And rsDet.EOF) Then
    rsDet.MoveFirst
    Do Until rsDet.EOF
        If rsDet!SpeciesID = lngSpecies And _
            Nz(rsDet!DetailID, 0) = Nz(vDet, 0) And _
            Nz(rsDet!Individuals, 1) = lngInd Then
            CheckMatch = True
            If Len(vCom) > 0 Then
                If rsDet!Comments <> vCom Then
                    rsDet.Edit
                    If Len(rsDet!Comments) > 0 Then
                        rsDet!Comments = rsDet!Comments & " " & vCom
                    Else
                        rsDet!Comments = vCom
                    End If
                    rsDet.Update
                End If
            End If
        Else
            If bDelete Then
                rsDet.Delete
            End If
        End If
        rsDet.MoveNext
    Loop
End If

rsDet.Close
Set rsDet = Nothing
Set db = Nothing

End Function

Private Sub SetFlag(lngImageID As Long)

DoCmd.RunSQL "UPDATE Photos " & _
    "Set NeedsUpdate=True " & _
    "WHERE ImageID=" & lngImageID

End Sub
