Private Sub ResizeForm(FormState As Integer)
'Show or hide renaming controls

Dim lngDetailHeight As Long
Dim ctrl As Control

On Error GoTo ErrHandler

'Determine detail height
If FormState = 1 Then
    lngDetailHeight = 1.125 * 1440
Else
    lngDetailHeight = 2.7083 * 1440
End If

'Show/hide controls based on their tag property
For Each ctrl In Me.Controls
    If Len(ctrl.Tag) > 0 Then
        If Left(ctrl.Tag, 1) > FormState Then
            ctrl.Height = 0
            ctrl.Top = lngDetailHeight
            ctrl.Visible = False
        Else
            ctrl.Height = Mid(ctrl.Tag, InStr(1, ctrl.Tag, "/") + 1) * 1440
            ctrl.Top = Mid(ctrl.Tag, 3, InStr(1, ctrl.Tag, "/") - 4) * 1440
            ctrl.Visible = True
        End If
    End If
Next ctrl

'Set detail height and window height
Me.Detail.Height = lngDetailHeight
Me.InsideHeight = lngDetailHeight + Me.FormFooter.Height + Me.FormHeader.Height
Me.Repaint
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description

End Sub

