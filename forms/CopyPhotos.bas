Private Function TestCPLocal() As Boolean
'Return true if table exists

Dim tdf As DAO.TableDef

For Each tdf In db.TableDefs
    If tdf.Name = "CPLocal" Then
        TestCPLocal = True
        Exit Function
    End If
Next

TestCPLocal = False

End Function

Private Function TestFiles() As Boolean
'Return true if destination folder exists
'and files are OK

Dim strMsg As String
Dim lngNextImg As Long

'Check for missing photos
rsPhotos.MoveFirst
Do Until rsPhotos.EOF
    If Len(Dir(rsPhotos!ImgPath)) = 0 Then
        strMsg = "Some of the photos you selected could not be found."
        GoTo TFExit
    Else
        rsPhotos.MoveNext
    End If
Loop

'Check destination folder
If Dir(NoSlash(strTargetPath), vbDirectory) = "" Then
    If MsgBox("The folder you specified doesn't exist." & vbNewLine & _
        vbNewLine & "Do you want to create it?", _
        vbYesNo, "Folder Not Found") = vbNo Then
        strMsg = "You chose to cancel the operation."
        GoTo TFExit
    Else
        MkDir NoSlash(strTargetPath)
    End If
Else
    If bModDB And (Dir(AddSlash(strTargetPath) & "*") <> "") Then
        strMsg = "Modules can only be created in a new, empty folder."
        GoTo TFExit
    End If
End If

'Check for pre-existing image files
If bRename Then
    lngNextImg = GetNextImageNumber(strTargetPath, strPrefix)
    If lngNextImg > 1 Then
        'Prompt to use next available number
        If MsgBox("The selected destination folder already " & _
            "contains files" & vbNewLine & "with the prefix '" & _
            strPrefix & ".'" & vbNewLine & vbNewLine & _
            "New files will be renamed starting with the next" & _
            vbNewLine & "available image number - " & lngNextImg & ".", _
            vbOKCancel, "OK to Continue?") = vbCancel Then
            strMsg = "You chose to cancel the operation."
            GoTo TFExit
        End If
    End If
End If

TestFiles = True

TFExit:
    'Show message if test failed
    If TestFiles = False Then
        MsgBox strMsg, vbExclamation, "Copy Canceled."
    End If

End Function

Private Function TestForm() As Boolean
'Check that form is OK to open

'Check for PhotoViewer form
If Not (IsLoaded("PhotoViewer") Or IsLoaded("Visits")) Then
    TestForm = False
    MsgBox "This form can only be opened using the copy " & _
        "button on the PhotoViewer form or the module button " & _
        "on the Visits form.", vbExclamation, "Canceled"
Else
    'Check that recordset can be initialized and contains records
    If TestRSPhotos Then
        TestForm = True
        SetVars
        'Set runtime DB option if opened from visits
        If Not IsLoaded("PhotoViewer") Then
            Me.chkDB = True
            chkDB_AfterUpdate
        End If
    Else
        TestForm = False
    End If
End If

End Function

Private Function TestModDups() As Boolean
'Check for duplicate filenames and prompt to rename

If bModDB And Not bRename Then
    If DCount("*", "CopyTestDupFileNames") = 0 Then
        TestModDups = True
    Else
        TestModDups = False
        MsgBox "You cannot include multiple files with the " & _
            vbNewLine & "same file name in a data module." & _
            vbNewLine & vbNewLine & "Please select different photos or " & _
            "select " & vbNewLine & "the option to rename " & _
            "the image files.", vbExclamation, "Copy canceled."
    End If
Else
    TestModDups = True
End If

End Function

Private Function TestRSPhotos() As Boolean
