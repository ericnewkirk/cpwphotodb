Private Sub cmdAdjustDetection_Click()
'Open form to choose a different photo as the start of the
'current detection event

Dim lngCurrentID As Long

On Error GoTo ErrHandler

'Get ID for record to edit
lngCurrentID = Me.IndDetectionID
'Open popup
DoCmd.OpenForm "AdjustIndDet", , , "IndDetectionID=" & lngCurrentID, _
    , acDialog, Me.IndDetFilmStrip.Form.ImageID
'Disable current event and requery form
bRequeryFlag = False
Me.Requery
'Enable current event
bRequeryFlag = True
'Move to original record
With Me.RecordsetClone
    .FindFirst "IndDetectionID=" & lngCurrentID
    If Not .NoMatch Then
        Me.Bookmark = .Bookmark
    Else
        'Stuck on first record - fire current event
        Call Form_Current
    End If
End With
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description
    'Enable current event
    bRequeryFlag = True

End Sub

Private Sub cmdDeleteDetection_Click()
'Mark detection deleted and move to previous detection

Dim lngDeleteID As Long

On Error GoTo ErrHandler

If MsgBox("Click OK if this sequence of photos is not " & _
    "independent of the preceding series." & _
    vbNewLine & vbNewLine & "All photos for this event will be added " & _
    "to the previous detection.", vbOKCancel, _
    "Are you sure?") = vbOK Then
    'Get ID for record to delete
    lngDeleteID = Me.IndDetectionID
    'Disable current event
    bRequeryFlag = False
    'Mark record deleted
    DeleteCurrentRecord (lngDeleteID)
    'Requery form
    Me.Requery
    'Enable current event
    bRequeryFlag = True
    'Move to previous record
    With Me.RecordsetClone
        .FindFirst "IndDetectionID=" & lngDeleteID
        If Not .NoMatch Then
            .MovePrevious
            Me.Bookmark = .Bookmark
        Else
            'Stuck on first record - fire current event
            Call Form_Current
        End If
    End With
End If
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description
    'Enable current event
    bRequeryFlag = True

End Sub

