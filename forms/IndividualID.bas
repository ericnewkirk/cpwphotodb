Private Sub FilterUpdate()
'Create and apply a filter string for the form

Dim strRecSource As String
Dim strFilterL As String
Dim strFilterS As String
Dim strFilterY As String
Dim strFilter As String

On Error GoTo ErrHandler

'Disable current event
bRequeryFlag = False
'Determine correct record source
If Me.chkNeedID Then
    strRecSource = "qryIndDetNeedID"
Else
    If Me.chkNeedTotals Then
        strRecSource = "qryIndDetNeedSummary"
    Else
        strRecSource = "qryIndDetRecSource"
    End If
End If

'Set record source if necessary
If Not Me.RecordSource = strRecSource Then
    Me.RecordSource = strRecSource
End If

'Create filter strings
If Not IsNull(Me.cboLocationFilter) Then
    strFilterL = "((LocationID)=" & _
        Me.cboLocationFilter & ") AND "
End If

If Not IsNull(Me.cboSpeciesFilter) Then
    strFilterS = "((SpeciesID)=" & _
        Me.cboSpeciesFilter & ") AND "
End If

If Not IsNull(Me.cboSeasonFilter) Then
    strFilterY = "((Year(ModifiedStart))=" & _
        Me.cboSeasonFilter & ")"
End If

'Combine filter strings
strFilter = TrimFilter(strFilter & strFilterL & strFilterS & _
    strFilterY, False)

'Apply filter
Me.Filter = strFilter
If Me.FilterOn = False Then
    Me.FilterOn = True
End If

'Update filter combo boxes
RowSourceUpdate strRecSource, strFilterL, strFilterS, strFilterY

'Enable current event and fire it
bRequeryFlag = True
Call Form_Current

Exit Sub

ErrHandler:
    ErrorMsg "An error occurred while applying your filter.", _
        Err.Number, Err.Description
    'Enable current event
    bRequeryFlag = True

End Sub

Private Sub FormatNavButtons()
'Update navigation bar based on current record

Dim bFirst As Boolean
Dim bLast As Boolean

On Error GoTo ErrHandler

'Ensure accurate record count
Me.RecordsetClone.MoveLast
DoEvents

'Check for first/last record
bFirst = (Me.Recordset.AbsolutePosition < 1)
bLast = (Me.CurrentRecord = Me.Recordset.RecordCount)
'Disable previous buttons on first record
Me.cmdNavBarPrevious.Enabled = Not bFirst
Me.cmdNavBarFirst.Enabled = Not bFirst
'Disable next buttons on last record
Me.cmdNavBarNext.Enabled = Not bLast
Me.cmdNavBarLast.Enabled = Not bLast

'Populate navigation bar text box
Me.txtRecordNumber = Me.CurrentRecord & " of " & _
    Me.Recordset.RecordCount

Exit Sub

ErrHandler:
    If Err.Number = 3021 Then   'No current record - filtered to nothing
        'Disable buttons
        Me.cmdNavBarFirst.Enabled = False
        Me.cmdNavBarPrevious.Enabled = False
        Me.cmdNavBarNext.Enabled = False
        Me.cmdNavBarLast.Enabled = False
        'Reset text box
        Me.txtRecordNumber = "0 of 0"
        Err.Clear
    Else
        If Err.Number = 2164 Then
            'Clicked on button before it got disabled
            Me.txtRecordNumber.SetFocus
            Resume
        Else
            ErrorMsg strGenericError, Err.Number, Err.Description
        End If
    End If

End Sub

Private Function InsertNewDetection() As Long
'Create a new detection event starting with current photo

Dim dStart As Date
Dim strValues As String
Dim strSQL As String

On Error GoTo ErrHandler

'Get start date
dStart = DLookup("ImageDate", "Photos", "ImageID=" & _
    Me.IndDetFilmStrip.Form.ImageID)
'Get values for other fields
strValues = Me.SpeciesID & ", " & Me.LocationID & ", #" & _
    USDate(dStart) & "#, #" & USDate(dStart) & "#"
'Insert new record
strSQL = "INSERT INTO IndependentDetections " & _
    "( SpeciesID, LocationID, DefaultStart, ModifiedStart ) " & _
    "VALUES ( " & strValues & " )"
DoCmd.SetWarnings False
DoCmd.RunSQL strSQL
'Return ID for new record
InsertNewDetection = DMax("IndDetectionID", "IndependentDetections")

InsertExit:
    DoCmd.SetWarnings True
    Exit Function

ErrHandler:
    ErrorMsg "Unable to add the detection.", _
        Err.Number, Err.Description
    Resume InsertExit

End Function

Private Sub RowSourceUpdate(strQry As String, strLoc As String, _
    strSp As String, strYr As String)

Dim strSelect As String
Dim strFrom As String
Dim strWhere As String
Dim strGroup As String
Dim strOrder As String
Dim strRS As String

'Update species combo
strSelect = "SELECT " & strQry & ".SpeciesID, Species.ShortName "
strFrom = "FROM " & strQry & " INNER JOIN Species ON " & _
    strQry & ".SpeciesID = Species.SpeciesID "
strWhere = TrimFilter(strLoc & strYr, True)
strGroup = Replace(strSelect, "SELECT", "GROUP BY")
strOrder = "ORDER BY Species.ShortName;"
strRS = strSelect & strFrom & strWhere & strGroup & strOrder
Me.cboSpeciesFilter.RowSource = strRS
Me.cboSpeciesFilter.Requery

'Update location combo
strSelect = "SELECT " & strQry & ".LocationID, " & _
    "StudyAreas.StudyAreaAbbr & ' - ' & CameraLocations.LocationName " & _
    " AS Location "
strFrom = "FROM (StudyAreas INNER JOIN CameraLocations " & _
    "ON StudyAreas.StudyAreaID = CameraLocations.StudyAreaID) " & _
    "INNER JOIN " & strQry & " ON CameraLocations.LocationID = " & _
    strQry & ".LocationID "
strWhere = TrimFilter(strSp & strYr, True)
strGroup = "GROUP BY " & strQry & ".LocationID, " & _
    "StudyAreas.StudyAreaAbbr & ' - ' & CameraLocations.LocationName "
strOrder = "ORDER BY StudyAreas.StudyAreaAbbr & ' - ' & " & _
    "CameraLocations.LocationName;"
strRS = strSelect & strFrom & strWhere & strGroup & strOrder
Me.cboLocationFilter.RowSource = strRS
Me.cboLocationFilter.Requery

'Update season combo
strSelect = "SELECT FieldSeason "
strFrom = "FROM " & strQry & " "
strWhere = TrimFilter(strLoc & strSp, True)
strGroup = "GROUP BY FieldSeason;"
strRS = strSelect & strFrom & strWhere & strGroup
Me.cboSeasonFilter.RowSource = strRS
Me.cboSeasonFilter.Requery

End Sub

Private Function TrimFilter(strIn As String, bAddWhere As Boolean) As String

Dim strResult As String

strResult = strIn
If Right(strResult, 5) = " AND " Then
    strResult = Left(strResult, Len(strResult) - 5)
End If

If bAddWhere And Len(strResult) > 0 Then
    strResult = "WHERE (" & strResult & ") "
End If

TrimFilter = strResult

End Function
