Private Sub Form_Current()
'Format nav bar and buttons based on current record
'Adjust button is handled in timer event to prevent lag

If bRequeryFlag Then
    'OK to continue
    FormatNavButtons
    Me.cboRefPhotos.Requery
    Me.cmdDeleteDetection.Enabled = CheckPreviousDetections
    Me.cmdAdjustDetection.Enabled = False
    Me.TimerInterval = 1000
End If

End Sub

Private Sub Form_Open(Cancel As Integer)
'Set requery flag and check for independent detections

'Enable current event
bRequeryFlag = True

'Check for detection records
If DCount("*", "IndependentDetections") = 0 Then
    'Check for popup
    If IsLoaded("AddIndDetections") Then
        Cancel = True
    Else
        Cancel = False
        'No records - display message and open form
        MsgBox "Before you can use the Individual ID form " & _
            "you need to assign independent detections.", , _
            "No Independent Detections"
        DoCmd.OpenForm "AddIndDetections", , , , , acDialog
        'Check for detection records again
        If DCount("*", "IndependentDetections") = 0 Then
            Cancel = True
        Else
            Cancel = False
        End If
    End If
End If

If Cancel = False Then
    'Minimize Switchboard
    HideSwitchboard acForm, "IndividualID"
End If

End Sub

Private Sub Form_Timer()
'Enable adjust button when photos are present before or after
'the start of this detection

On Error Resume Next

'Disable event once it's fired
Me.TimerInterval = 0
'Enable or disable button based on criteria
Me.cmdAdjustDetection.Enabled = (CheckPreviousDetections Or _
    (Me.IndDetFilmStrip.Form.Recordset.RecordCount > 1))

End Sub

Private Sub Form_Unload(Cancel As Integer)
'Close reference photos popup and restore switchboard

If IsLoaded("IndRefPhotos") Then
    DoCmd.Close acForm, "IndRefPhotos"
End If
RestoreSwitchboard

End Sub

