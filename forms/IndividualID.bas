    bRequeryFlag = True
    With Me.RecordsetClone
        .FindFirst "IndDetectionID=" & lngNewID
        If Not .NoMatch Then
            Me.Bookmark = .Bookmark
        Else
            'Stuck on first record - fire current event
            Call Form_Current
        End If
    End With
End If
Exit Sub

