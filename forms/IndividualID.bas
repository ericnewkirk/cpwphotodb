Private Function CheckPreviousDetections() As Boolean
'Check for previous detections of same species/location

CheckPreviousDetections = Not IsNull(Me.PrevDetection)

End Function

Private Sub DeleteCurrentRecord(lngID As Long)
'Delete current detection event, combining it with the one before

Dim strSQL As String

On Error GoTo ErrHandler

strSQL = "UPDATE IndependentDetections SET Deleted=True " & _
        "WHERE IndDetectionID=" & lngID
DoCmd.SetWarnings False
DoCmd.RunSQL strSQL

DeleteExit:
