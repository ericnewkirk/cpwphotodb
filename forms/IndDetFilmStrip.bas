Private Sub Form_Current()
'Enables split button on parent form when current
'photo is not the first one

Call Form_IndividualID.EnableSplitButton(Not FirstPhoto())
Me.Parent.lblPhoto.Caption = "Current Photo: " & Me.FileName & _
    "   (" & Me.ImageDate & ")"

End Sub

Public Function FirstPhoto() As Boolean
'Returns true if current photo is first one

FirstPhoto = Me.Recordset.AbsolutePosition < 1

End Function
