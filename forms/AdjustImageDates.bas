Private Sub PreviewDate()

If (IsNull(Me.cboAdjType) Or _
    IsNull(Me.cboInterval) Or _
    IsNull(Me.txtNumber)) Then
    Me.txtNewFirstPhoto = Null
    Me.cmdGo.Enabled = False
Else
    Me.txtNewFirstPhoto = DateAdd(Me.cboInterval, _
        CDbl(Me.cboAdjType & Me.txtNumber), Me.txtFirstPhoto)
    Me.cmdGo.Enabled = True
End If

End Sub
