Private Sub cmdGo_Click()

Dim strSQL As String

On Error GoTo ErrHandler

If MsgBox("Are you sure you want to edit the metadata " & _
    "for all photos associated with this visit?", _
    vbOKCancel, "Confirm Update") = vbCancel Then
        Exit Sub
End If

DoCmd.SetWarnings False
strSQL = "UPDATE Photos " & _
    "SET ImageDate = DATEADD('" & Me.cboInterval & _
    "', " & CDbl(Me.cboAdjType & Me.txtNumber) & ", ImageDate) " & _
    "WHERE (VisitID = " & Me.VisitID & ");"
DoCmd.RunSQL strSQL

DoCmd.Close

cmdGoExit:
    DoCmd.SetWarnings True
    Exit Sub

ErrHandler:
    ErrorMsg "An error occurred performing the update.", _
        Err.Number, Err.Description, Me.Name
    Resume cmdGoExit

End Sub

