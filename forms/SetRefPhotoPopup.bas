Private Sub Form_Open(Cancel As Integer)
'Prevent opening form without parameters

If IsNull(Me.Filter) Or IsNull(Me.OpenArgs) Then
    MsgBox "This form can only be opened from the Individual ID form."
    Cancel = True
Else
    Cancel = False
End If

End Sub
