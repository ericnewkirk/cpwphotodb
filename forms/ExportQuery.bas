Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database
Option Explicit

Private Sub cmdBrowsePath_Click()
'Opens 'save as' dialog box

On Error GoTo ErrHandler

Dim dlg As Object
Dim strFilePath As String

Set dlg = Application.FileDialog(msoFileDialogSaveAs)
If IsNull(Me.txtPath) Then
    strFilePath = "MyQueryResults."
    If Me.optFormat = 2 Then
        strFilePath = strFilePath & Me.Extension
    Else
        strFilePath = strFilePath & "xlsx"
    End If
Else
    strFilePath = Me.txtPath
End If

With dlg
    .InitialFileName = strFilePath
    .Title = "Save query results as..."
    .ButtonName = "Continue"
    If .Show = -1 Then
        strFilePath = dlg.SelectedItems(1)
        Me.txtPath = strFilePath
    End If
End With

DlgExit:
    Set dlg = Nothing
    Call txtPath_AfterUpdate
    Exit Sub

ErrHandler:
    ErrorMsg "An error occurred in the dialog box.", _
        Err.Number, Err.Description
    Me.txtPath = Null
    Resume DlgExit

End Sub

Private Sub cmdCancel_Click()
'Close export form

On Error Resume Next
DoCmd.Close

End Sub

Private Sub cmdExport_Click()
'Exports query results to excel or text file

On Error GoTo ErrHandler

'Check for valid output path
If TestPaths = True Then
    'Disable controls
    DoCmd.Hourglass True
    LockForm
    'Export to file
    ExportQuery
    'Close export form
    LockForm False
    DoCmd.Close acForm, Me.Name
    DoCmd.Hourglass False
Else
    MsgBox "The location you specified doesn't exist.", _
        vbExclamation, "Export Canceled"
End If

Exit Sub

ErrHandler:
    ErrorMsg "The export operation encountered an error.", _
        Err.Number, Err.Description, "Export Error"
    AccessCloseButtonEnabled True
    DoCmd.Hourglass False

End Sub

Private Sub cmdHelp_Click()
'Display help message

HelpMsg Me.Name, Me.Name & " Form Help"

End Sub

Private Sub Form_Open(Cancel As Integer)
'Check filter and set second format option

If Len(Me.Filter) = 0 Then
    MsgBox "The export form can only be opened from one of the " & _
        "query builder forms."
    Cancel = True
Else
    If IsNull(Me.Program) Then
        Me.Option65.Enabled = False
        Me.Label66.Caption = ""
    Else
        Me.Label66.Caption = Me.Program & " input file (." & _
            Me.Extension & ")"
    End If
    Me.Repaint
End If

End Sub

Private Sub optFormat_AfterUpdate()
'Updates filename extension when format is changed

On Error Resume Next

If Not IsNull(Me.txtPath) Then
    Me.txtPath = AddExtension(Me.txtPath)
End If

End Sub

Private Sub txtPath_AfterUpdate()
'Enables export button and checks file extension
'when a filename has been entered

On Error Resume Next

If IsNull(Me.txtPath) Then
    Me.cmdExport.Enabled = False
Else
    Me.cmdExport.Enabled = True
    Me.txtPath = AddExtension(Me.txtPath)
End If

End Sub

'------------------------------
'Form-specific custom functions
'------------------------------

Private Function AddExtension(strPath As String) As String
'Adds correct extension to output file name

On Error GoTo ErrHandler

Dim iDot As Integer
Dim strNewPath As String

'Remove current extension if there is one
iDot = InStr(1, strPath, ".")
If iDot > 0 Then
    strNewPath = Left(strPath, iDot)
Else
    strNewPath = strPath & "."
End If

'Add appropriate extension based on format selected
If Me.optFormat = 1 Then
    strNewPath = strNewPath & "xlsx"
Else
    strNewPath = strNewPath & Me.Extension
End If

AddExtension = strNewPath
Exit Function

ErrHandler:
    AddExtension = strPath

End Function

Private Sub ExportQuery()

Dim strDest As String
Dim strSettings As String
Dim strQry As String
Dim bView As Boolean

'Get required info
strDest = Me.txtPath
If Not IsNull(Me.OpenArgs) Then
    strSettings = "Settings: " & Me.OpenArgs
End If
bView = Me.chkView
strQry = Me.QueryName

If Me.optFormat = 1 Then   'Create spreadsheet
    ExportToExcel strQry, strSettings, strDest, bView
Else            'Create text file
    Select Case strQry
        Case "SECRQuery"
            MsgBox "Make sure to use" & vbNewLine & vbNewLine & _
                "   binary.usage = FALSE" & vbNewLine & _
                vbNewLine & "when importing the data into R."
            ExportForSECR strQry, strSettings, strDest, bView
            ExportDetectors strDest, strSettings, bView
        Case "ActivityPatternQuery"
            MsgBox "Make sure to use" & vbNewLine & vbNewLine & _
                "   comment.char = '#'" & vbNewLine & vbNewLine & _
                "when importing the data into R."
            ExportForOverlap strQry, strSettings, strDest, bView
        Case "OccupancyQuery"
            ExportForMARK strQry, strSettings, strDest, bView, True
        Case "MarkRecaptureQuery"
            ExportForMARK strQry, strSettings, strDest, bView, False
        Case "qryCameraLocationsLatLong"
            ExportKML strQry, strDest, "Latitude", "Longitude", _
                Me.chkView, "LocationName", "AccessNotes"
        Case "qryResultsByLocationLatLong"
            ExportKML strQry, strDest, "Latitude", "Longitude", _
                Me.chkView, "LocationName"
        Case Else
            ExportCSV strQry, strDest, bView
    End Select
End If

End Sub

Private Sub LockForm(Optional bLocked As Boolean = True)
'Disables controls and close button while code is running

On Error Resume Next

Me.txtFocus.SetFocus
AccessCloseButtonEnabled Not bLocked
Me.cmdExport.Enabled = Not bLocked
Me.cmdCancel.Enabled = Not bLocked
Me.txtPath.Enabled = Not bLocked
Me.cmdBrowsePath.Enabled = Not bLocked
Me.lblStatus.Visible = bLocked
Me.Repaint
DoEvents

End Sub

Private Function TestPaths() As Boolean
'Test for valid output path

Dim strPath As String

On Error Resume Next

TestPaths = False

If InStr(1, Me.txtPath, "\") > 0 Then
    strPath = Left(Me.txtPath, InStrRev(Me.txtPath, _
        "\"))
        If FileExists(strPath, True) Then
            TestPaths = True
        End If
End If

End Function
