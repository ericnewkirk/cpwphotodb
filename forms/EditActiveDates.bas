Option Compare Database
Option Explicit

Private Sub cmdHelp_Click()
'Display help message

HelpMsg Me.Name, "Active Dates Help"

End Sub

Private Sub linkDefault_Click()
'Set dates to default values

If MsgBox("Are you sure you want to change the active dates " & _
    "for this visit?", vbOKCancel) = vbOK Then
    Me.ActiveStart = Me.FirstPhoto
    Me.ActiveEnd = Me.LastPhoto
End If

End Sub
