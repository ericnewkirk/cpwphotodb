Private Sub cboFirstImage_AfterUpdate()
'Enable/disable apply button

Me.cmdApply.Enabled = Not IsNull(Me.cboFirstImage)

End Sub

Private Sub cmdApply_Click()

Dim dNewStart As Date

On Error GoTo ErrHandler

dNewStart = Me.cboFirstImage.Column(4)

Me.ModifiedStart = dNewStart

DoCmd.Close acForm, "AdjustIndDet", acSaveNo

ApplyExit:
    Exit Sub

ErrHandler:
    ErrorMsg "Unable to adjust detection.", Err.Number, Err.Description
    Resume ApplyExit

End Sub

Private Sub cmdCancel_Click()
'Close

DoCmd.Close acForm, "AdjustIndDet"

End Sub

Private Sub cmdHelp_Click()
'Display help message

HelpMsg Me.Name, "Independent Detections"

End Sub

Private Sub Form_Open(Cancel As Integer)
'Cancel if not coming from PhotoID form

On Error GoTo ErrHandler

If IsLoaded("IndividualID") Then
    Cancel = False
Else
    Cancel = True
    MsgBox "This form can only be accessed from the IndividualID Form."
    Exit Sub
End If

Me.cboFirstImage.RowSource = "SELECT qryAdjustIndDetRecSource.ImageID, " & _
    "qryAdjustIndDetRecSource.FileName, qryAdjustIndDetRecSource.Current, " & _
    "qryAdjustIndDetRecSource.ImgPath, qryAdjustIndDetRecSource.ImageDate " & _
    "FROM qryAdjustIndDetRecSource WHERE " & _
    "(((qryAdjustIndDetRecSource.IndDetectionID)=" & Me.IndDetectionID & _
    ")) ORDER BY qryAdjustIndDetRecSource.FileName;"

'Set combo box to current photo
If Len(Me.OpenArgs) > 0 Then
    Me.cboFirstImage = CLng(Me.OpenArgs)
    Me.cmdApply.Enabled = True
End If

Exit Sub

ErrHandler:
    ErrorMsg "An error occured attempting to open the form.", _
        Err.Number, Err.Description

End Sub
