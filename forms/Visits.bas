Option Compare Database
Option Explicit

Private Const strGenericError As String = _
    "The Camera data form encountered an error."

Private Sub cmdHelp_Click()
'Display help message

HelpMsg Me.Name, "Camera Visit Data Help"

End Sub

Private Sub Form_Load()
'Display current location in combo box

On Error GoTo ErrHandler

Me.LocationName = Me.LocationID
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description, Me.Name

End Sub

Private Sub LocationName_AfterUpdate()
'Move to location selected

On Error GoTo ErrHandler

If Not IsNull(Me.LocationName) Then
    'Search for the location
    With Me.RecordsetClone
        .FindFirst "[LocationID] = " & Me.LocationName
        If Not .NoMatch Then
            'Jump to that record
            Me.Bookmark = .Bookmark
        End If
    End With
End If
Exit Sub

ErrHandler:
    ErrorMsg strGenericError, Err.Number, Err.Description, Me.Name

End Sub
